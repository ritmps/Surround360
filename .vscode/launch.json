{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/surround360_camera_ctl/build/bin/CameraControl",
        "args": ["--props", "--debug"],
        // "args": ["--list", "--debug"],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build",
        "miDebuggerPath": "/usr/bin/gdb",
        
        "logging": {
          "moduleLoad": false,
          "trace": true,
          "engineLogging": false,
          "programOutput": true,
          "exceptions": true
        }
      }
    ]
  }